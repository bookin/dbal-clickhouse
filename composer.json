{
    "name": "bookin/dbal-clickhouse",
    "type": "library",
    "version" : "1.1.0",
    "description": "Doctrine DBAL driver for ClickHouse",
    "keywords": ["doctrine", "dbal", "clickhouse", "driver"],
    "homepage": "http://github.com/bookin/dbal-clickhouse",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochalygin Aleksandr",
            "email": "a@mochalygin.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "smi2/phpClickHouse": "^0.17",
        "doctrine/dbal": ">=2.5"
    },
    "autoload": {
        "psr-4": { "FOD\\DBALClickHouse\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "FOD\\DBALClickHouse\\Tests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    }
}
